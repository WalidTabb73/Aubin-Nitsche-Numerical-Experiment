   ],
   "source": [
    "M = 500\n",
    "N = 5\n",
    "\n",
    "c = solve_problem(M, N)\n",
    "\n",
    "grid = np.linspace(0,1,1000)\n",
    "u_exact = u(grid)\n",
    "u_approx = np.array([phi(m,N) @ c for m in grid])\n",
    "error = np.abs(u_exact - u_approx)\n",
    "\n",
    "plt.figure()\n",
    "plt.plot(grid, u_exact, label=\"Exact\")\n",
    "plt.plot(grid, u_approx, \"--\", label=\"Approx\")\n",
    "plt.legend()\n",
    "plt.title(\"Exact vs Approx\")\n",
    "plt.show()\n",
    "\n",
    "plt.figure()\n",
    "plt.plot(grid, error)\n",
    "plt.title(\"Pointwise Error\")\n",
    "plt.show()"
   ]
  },
