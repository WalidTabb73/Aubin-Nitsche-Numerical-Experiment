    "import numpy as np\n",
    "import matplotlib.pyplot as plt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "04b03a65-1475-409b-9048-0e3d53afdd79",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Target function (replace with your u(m))\n",
    "def u(m):\n",
    "    return np.sin(2*np.pi*m)\n",
    "\n",
    "def du(m):\n",
    "    return 2*np.pi*np.cos(2*np.pi*m)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "bc18c8bf-c00e-44e0-a1f2-56c9410d5e70",
   "metadata": {},
   "outputs": [],
   "source": [
    "def phi(m, N):\n",
    "    # basis vector [1, m, m^2, ..., m^N]\n",
    "    return np.array([m**k for k in range(N+1)])\n",
    "\n",
    "def dphi(m, N):\n",
    "    # derivative of basis\n",
    "    d = []\n",
    "    for k in range(N+1):\n",
    "        if k == 0:\n",
    "            d.append(0.0)\n",
    "        else:\n",
    "            d.append(k * m**(k-1))\n",
    "    return np.array(d)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "7a584c3f-2171-44a7-8b3e-cb9cf717bc35",
   "metadata": {},
   "outputs": [],
   "source": [
    "def sample_points(M):\n",
    "    return np.random.rand(M)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "a819ecc8-baf3-4093-a546-8bd547c79995",
   "metadata": {},
   "outputs": [],
   "source": [
    "def build_system(M, N):\n",
    "    m_samples = sample_points(M)\n",
    "    \n",
    "    A_rows = []\n",
    "    b_rows = []\n",
    "    \n",
    "    for m in m_samples:\n",
    "        # function row\n",
    "        A_rows.append(phi(m, N))\n",
    "        b_rows.append(u(m))\n",
    "        \n",
    "        # derivative row\n",
    "        A_rows.append(dphi(m, N))\n",
    "        b_rows.append(du(m))\n",
    "    \n",
    "    A = np.vstack(A_rows)\n",
    "    b = np.array(b_rows)\n",
    "    \n",
    "    return A, b"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "0e6480aa-9489-4899-b1eb-fd190a6ae544",
   "metadata": {},
   "outputs": [],
   "source": [
    "def solve_problem(M, N):\n",
    "    A, b = build_system(M, N)\n",
    "    c, *_ = np.linalg.lstsq(A, b, rcond=None)\n",
    "    return c"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "80b2059f-4bce-42e4-9242-ddc12fa2c4c9",
   "metadata": {},
   "outputs": [
