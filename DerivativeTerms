   "source": [
    "Ns = range(1,15)\n",
    "errors = []\n",
    "\n",
    "for N in Ns:\n",
    "    c = solve_problem(1000, N)\n",
    "    grid = np.linspace(0,1,1000)\n",
    "    u_exact = u(grid)\n",
    "    u_approx = np.array([phi(m,N) @ c for m in grid])\n",
    "    errors.append(np.max(np.abs(u_exact - u_approx)))\n",
    "\n",
    "plt.figure()\n",
    "plt.semilogy(Ns, errors, marker=\"o\")\n",
    "plt.title(\"Max Error vs Basis Degree\")\n",
    "plt.xlabel(\"N\")\n",
    "plt.ylabel(\"max error\")\n",
    "plt.show()"
   ]
